    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "# Create a sample image (white square)\n",
    "image = np.ones((100, 100), dtype=np.uint8) * 255\n",
    "\n",
    "# Resize the image\n",
    "resized_image = cv2.resize(image, (28, 28))\n",
    "print(\"Resized image shape:\", resized_image.shape)\n"
   ]
